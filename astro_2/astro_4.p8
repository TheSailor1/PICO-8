function drw_ast(_x,_y,_s)
	local bx,by,bw,bh,hx,hy,hw,hh
	
	if _s=="st" then
		--stand
		bx,by,bw,bh=0,0,12,9
		hx,hy,hw,hh=0,11,12,10
	elseif _s=="ex" then
	--exclamation
		bx,by,bw,bh=12,0,12,9
		hx,hy,hw,hh=12,11,12,10
	else
	
	end
	
	--body
	sspr(bx,by,bw,bh,_x,_y)
	--head
	sspr(hx,hy,hw,hh,_x+1,(_y-7)+sin(tmr*0.004)*0.2)
end

function drw_hud()
	rrect2(1,91,125,34,8)
	
	
	--oxy
	rrect2(3,101,33,21,8)
	rectfill(32,120,35,122,8)
	rrect2(33,98,8,5,0)
	sspr(99,0,11,10,32,96)
