function upd_tmr()
	if t>31000 then
		t=0
	else
		t+=1
	end
end

function lerp(f,t,w)
	return (t-f)/w
end

function easeoutbounce(t)
    local n1=7.5625
    local d1=2.75

    if (t<1/d1) then
        return n1*t*t;
    elseif(t<2/d1) then
        t-=1.5/d1
        return n1*t*t+.75;
    elseif(t<2.5/d1) then
        t-=2.25/d1
        return n1*t*t+.9375;
    else
        t-=2.625/d1
        return n1*t*t+.984375;
    end
end


