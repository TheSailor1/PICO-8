function drw_menu()
	cls()
	
	?sp_men,0,0
	
	ly=easeoutbounce(anitmr)*120
--	?sp_log,28,-60+ly
	?sp_log,-70+ly,68
	
	if ly==120 then
		?"play",14,94,8
		?"worlds",14,102,8
		?"v2.0",2,2,5
	end
	
end

function drw_intro()
	ly=easeinoutquad(anitmr)*90
	
	cls()
	drw_star(ly)
	?sp_p1,85,100-ly
	
	map(0,0,mapx,200-ly,17,2)
	
	bods={"st","st","ex","st","ex","ex","st","ex"}
	drw_ast(32,191-ly,bods[txt_cnt])
	drw_ali(60,190-ly)
	
	txt={
		"so i stood there...\nlooking her in the eye.\nand i said to her,\n\"don't touch my cookies!\"",
		"[alien grunt noises]",
		"i know, right?!\nthe absolute nerve of her!",
		"[radio] ksst...\ndave...\nwhere are you??",
		"[sigh]\ni'm doing science, lavern\nvery technical.\nyou won't understand it.",
		"[radio] dave...\nwe're ready to launch.\ndid you not get the memo?",
		"dang it!\ni got to go alien!",
		"[alien grunt noises]",
	}
	txt_x={10,30,10,10,10,10,10,30}
	txt_y={60,80,70,30,60,30,70,80}
	txt_c={5,2,5,1,5,1,5,2}
	bub_x={8,28,8,8,8,8,8,28}
	bub_y={59,79,69,29,59,29,69,79}
	bub_w={102,82,105,62,102,102,73,82}
	bub_h={24,6,12,18,24,18,12,6}
	bub_c={15,9,15,8,15,8,15,9}
