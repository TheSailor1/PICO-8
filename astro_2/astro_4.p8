--particles

function ini_parts()
	stars={}
end

function ad_star(_y,_amt)
	stars={}
	for i=1,_amt do
		add(stars, {
			x=64+rnd(64)-rnd(64),
			y=_y+rnd(64)-rnd(64),
			r=0,
			c=rnd({1,1,1,2,9,12})
		})
	end
end

function drw_star(_o)
	for s in all(stars) do
		circfill(s.x,s.y-_o,s.r,s.c)
	end
end
-->8
-- astro

function drw_ast(_x,_y,_s)
	local bx,by,bw,bh,hx,hy,hw,hh
	
	if _s=="st" then
		--stand
		bx,by,bw,bh=0,0,12,9
		hx,hy,hw,hh=0,11,12,10
	elseif _s=="ex" then
	--exclamation
		bx,by,bw,bh=12,0,12,9
		hx,hy,hw,hh=12,11,12,10
	else
	
	end
	
	--body
	sspr(bx,by,bw,bh,_x,_y)
	--head
	sspr(hx,hy,hw,hh,_x+1,(_y-7)+sin(tmr*0.004)*0.2)
end
