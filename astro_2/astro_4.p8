function _init()
	tmr,anitmr,wtmr,ly=0,0,0,0 
	ini_menu()
	ini_parts()
end

function ini_menu()
	new_scn(upd_menu,drw_menu)
end

function ini_intro()
	wtmr=120
	anitmr=0
	txt_cnt=1
	
	ad_star(127,100)
	
	new_scn(upd_intro,drw_intro)
end

function ini_game()
	tmr=0
	mapx=0
	mapspd=1
	plx=85
	ad_star(127,100)
	new_scn(upd_game,drw_game)
end

