function upd_menu()
	upd_tmr()
	upd_wtmr()
	upd_anitmr()
	upd_tick()
	upd_meteors()
	
	if btnp(🅾️) then
		sfx"63"
		ini_intro()
	end
end

function upd_intro()
	upd_tmr()
	upd_wtmr()
	upd_anitmr()
	upd_tick()
	upd_meteors()
	
	if btnp(🅾️) and wtmr==0 and txt_cnt<#txt then
		txt_cnt+=1
		wtmr=40
		sfx"63"
		if txt_cnt==4 or txt_cnt==6 then
			sfx"62"
		elseif txt_cnt==#txt then
			ini_game()
		end
	end
end

function upd_game()
	upd_tmr()
	if moving then
		plx-=(mapspd/600)
		mapx=(mapx-1)%128
	end
	
	
	upd_tick()
	upd_meteors()
end
