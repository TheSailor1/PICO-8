--particles

function ini_parts()
	stars={}
end

function ad_star(_y,_amt)
	stars={}
	for i=1,_amt do
		add(stars, {
			x=64+rnd(64)-rnd(64),
			y=_y+rnd(64)-rnd(64),
			r=0,
			c=rnd({1,1,1,2,9,12})
		})
	end
end

function drw_star(_o)
	for s in all(stars) do
		circfill(s.x,s.y-_o,s.r,s.c)
	end
end

function ini_meteors()
	meteors={}
end

function new_meteor()
	add(meteors,{
		x=rnd(127),
		y=rnd(80),
		c=rnd({1,13,12}),
		r=rnd({0,1}),
		spdx=rnd({-3,-2,-1,1,2,3}),
		spdy=rnd({-3,-2,-1,1,2,3})
	})
end

function upd_meteors()
	for m in all(meteors) do
		m.x+=m.spdx
		m.y-=m.spdy
		
		--if offscreen
		if m.y<0 or m.x<0 or
					m.y>80 or m.x>127 then
						del(meteors,m)
		end
	end
end

function drw_meteors()
	for m in all(meteors) do
		circfill(m.x,m.y,m.r,m.c)
	end
end
