function ini_meteors()
	meteors={}
end

function new_meteor()
	local m
	
	m={
		x=rnd(127),
		y=rnd(80),
		c=rnd({1,13,12}),
		r=rnd({0,1}),
		spdx=rnd({-3,-2,-1,1,2,3}),
		spdy=rnd({-3,-2,-1,1,2,3})
	}
	add(meteors,m)
end

function upd_meteors()
	local m
	for m in all(meteors) do
		m.x+=m.spdx
		m.y-=m.spdy
		
		--if offscreen
		if m.y<0 or m.x<0 or
					m.y>80 or m.x>127 then
						del(meteors,m)
		end
	end
end

function drw_meteors()
	local m
	for m in all(meteors) do
		circfill(
			m.x,
			m.y,
			m.r,
			m.c
		)
	end
	
	
end
