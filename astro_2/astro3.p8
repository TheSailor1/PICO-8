	local _o,_y,_c,i
	local num
	if rnd(1)<0.1 then
		num=4
	elseif rnd(1)<0.3 then
		num=3
	elseif rnd(1)<0.5 then
		num=2
	else
		num=1
	end
	
	
	local pickup=false
	if plr.boosts<plr.boosts_max and
				rnd(1)<0.1 then
		pickup=true
	end
	
	if pickup==false then
		local choice=rnd({0,1,0})
		
		for i=1,num do
			if choice==0 then
				_y=ground-14
				_c=11
			else
				_y=ground-26
				_c=8
			end
		
			_o={
				x=128+(i*20),
				y=_y,
				w=16,
				h=7,
				t=choice,
				flg=true
			}
			add(objs,_o)
		end--for i
	else
		_o={
			x=128,
			y=ground-12,
			w=6,
			h=8,
			t=2, --pickup
			flg=false
		}
		add(objs,_o)
	end--if pickup
end

function upd_objs()
	local _o
	
	if gap_tmr>0 then
		gap_tmr-=1
	else
		gap_tmr=0
	end
	
	for _o in all(objs) do
		_o.x-=plr.spd
		if _o.x<=-20 then
			if _o.flg==true then
				flg_cnt+=1
				--dist meter in hud
				if gap_tmr==0 then
					s_line+=tot_dist
					gap_tmr=10
				end
			end
			del(objs,_o)
			if #objs==0 then
				spwn_obj()
			end
		end
	end
end

function drw_objs()
	local _o
	
	head_t+=1
	if head_t>=15 then
		grd_c+=1
		if grd_c>#grnd_head_ani then
			grd_c=1
		end
		head_t=0
	end
	
	for _o in all(objs) do
		if _o.t==0 then
			sspr(grnd_b_ani[grd_c][1],
								grnd_b_ani[grd_c][2],
								grnd_b_ani[grd_c][3],
								grnd_b_ani[grd_c][4],
								_o.x,_o.y+2
			)
						sspr(grnd_head_ani[grd_c][1],
											grnd_head_ani[grd_c][2],
											grnd_head_ani[grd_c][3],
											grnd_head_ani[grd_c][4],
											_o.x,_o.y+sin(t/60)
			)
			
		elseif _o.t==1 then
						sspr(air_b_ani[grd_c][1],
											air_b_ani[grd_c][2],
											air_b_ani[grd_c][3],
											air_b_ani[grd_c][4],
											_o.x+4,_o.y+sin(t/50) 
										)
						sspr(air_h_ani[grd_c][1],
											air_h_ani[grd_c][2],
											air_h_ani[grd_c][3],
											air_h_ani[grd_c][4],
											_o.x,_o.y+sin(t/60)
										)
		elseif _o.t==2 then
			sspr(112,32,_o.w,_o.h,_o.x,_o.y+cos(time()*2)*2)
		end
	end
	
end
