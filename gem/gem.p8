function opentile()
	local t
	for t=1,#tiles do
		if (btnp(‚ùé)) then
			if (curx*size==tiles[t].id_x
			and cury*size==tiles[t].id_y 
			and not tiles[t].revealed) then
				tiles[t].revealed=true
				checkaround(t)
			end
			
		end--if
	end
end--opentile

