-- game play

function ini_board()
	
	t=0
	gtimer=0
	timer=0
	
	tiles={}
	flags={}
	
	
	cols=8
	rows=8
	size=12
	
	--cursor
	curx=0
	cury=0
	curt="dig"
	
	local i,j
	for i=0,cols-1 do
		for j=0,rows-1 do
			local c={
				id_x=i*size,
				id_y=j*size,
				revealed=false,
				hasmine=false,
				warn=0,
				flag=false,
				spx=rnd({0,0,0,0,0,0,0,0,12,24,36}),
				spy=0
			}
			add(tiles,c)
		end--for j
	end--for i
	
	--adding mines
	allmines=10
	allflags=10
	plrflags=allflags
	
	while allmines>0 do
		local rtile=flr(1+rnd(#tiles-1))
		if not tiles[rtile].hasmine then
			tiles[rtile].hasmine=true
			allmines-=1
		end
	end
	
end--ini_board()

function drw_bkg()
	rectfill(0,117,127,127,1)
end

function drw_board()
	local t
	for t in all(tiles) do
		if t.revealed and t.hasmine then
			t.spx=11
			t.spy=109
		elseif t.revealed then
			t.spx=48
		end
		
		rectfill(4+t.id_x,4+t.id_y,96,96,1)
		
		local i
		for i=0,15 do
			pal(i,1)
		end
		sspr(t.spx,t.spy,12,13,4+t.id_x,4+t.id_y)
		pal()
		
		
		sspr(t.spx,t.spy,12,13,2+t.id_x,2+t.id_y)
			
		if t.revealed and
		not t.hasmine and
			t.warn>0 then
				print(t.warn,6+t.id_x,6+t.id_y,9)
		end
		
	end--for
end--drw_board()

function drw_cursor()
	local c,t
	for c=0,15 do
		pal(c,1)
	end
	sspr(23,109,16,16,1+curx*size,2+cury*size)
	pal()
	
	for t in all(tiles) do
		if curx*size==t.id_x and
		cury*size==t.id_y then
			if t.revealed then
				if t.flag then
					pal(9,13)
					pal(10,12)
					pal(4,7)
					sspr(23,109,16,16,curx*size,cury*size)
					pal()
				else
					sspr(23,109,16,16,curx*size,cury*size)
				end
			else
				if curt=="dig" then
					sspr(61,14,16,16,curx*size,cury*size)
				elseif curt=="flag" then
					sspr(45,14,16,16,curx*size,cury*size)
				end
			end
		end
	end
	
end--drw_cursor()

