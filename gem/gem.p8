	if btnp(‚ùé) then
		if curt=="dig" then
			opentile()
		elseif curt=="flag" then
			flagtile()
		end
	end
end--upd_cursor()

function opentile()
	local t
	for t=1,#tiles do
			if (curx*size==tiles[t].id_x
			and cury*size==tiles[t].id_y 
			and not tiles[t].revealed) then
				tiles[t].revealed=true
				checkaround(t)
			end
	end
end--opentile

function flagtile()
	local t
	for t=1,#tiles do
			if (curx*size==tiles[t].id_x
			and cury*size==tiles[t].id_y) then 
				if tiles[t].revealed and
				tiles[t].flag then
					tiles[t].flag=false
					plrflags+=1
					local f
					for f in all(flags) do
						if tiles[t].id_x==f.x and
						tiles[t].id_y==f.y then
							del(flags,f)
						end
					end
				end
				
				if not tiles[t].revealed 
				and not tiles[t].flag and
				plrflags>0 then
					add(flags,{
						x=curx*size,
						y=cury*size
						})
					tiles[t].flag=true
					plrflags-=1
				end
			end
	end
end--flagtile

function drw_flags()
