	t+=1
	
	if timer<999 then
		gtimer+=1
	end
	
	if (gtimer>=60) then
	 gtimer=0
	 timer+=1
	end
	
	upd_cursor()
end

function drw_game()
	cls(12)
	drw_bkg()
