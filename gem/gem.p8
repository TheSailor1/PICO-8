function movecursor()
	if btnp(‚û°Ô∏è) then
		if curx<cols-1 then
			curx+=1
		else
			curx=0
		end
	end
	if btnp(‚¨ÖÔ∏è) then
		if curx>0 then
			curx-=1
		else
			curx=cols-1
		end
	end
	if btnp(‚¨ÜÔ∏è) then
		if cury>0 then
			cury-=1
		else
			cury=rows-1
		end
	end
	if btnp(‚¨áÔ∏è) then
		if cury<rows-1 then
			cury+=1
		else
			cury=0
		end
	end
end--movecursor()
