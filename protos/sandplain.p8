pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
function _init()
	t=0
end

function _update60()
	t+=1/10
end

function _draw()
	cls(12)
	for i=-64,64 do
		for j=0,64 do
			c=i/(2+abs(j/1.5))+sin(j/32)
			pset
			(
				i+64,j+64,
				9+(flr((c+t)/1))%2
			)
		end
	end
end
