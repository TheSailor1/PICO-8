pico-8 cartridge // http://www.pico-8.com
version 43
__lua__
--mana
--thesailor

function _init()
	ini_dice()
	upd=upd_game
	drw=drw_game
end

function _update60()
	upd()
end

function _draw()
	drw()
end
-->8
--draws
function drw_game()
	cls(13)
	drw_dice()
end
-->8
--updates
function upd_game()
	if btnp(4) then
		rolldice(3)
	end
end
-->8
--dice
function ini_dice()
	dices={}
	rolldice(3)
end

function rolldice(num)
	for i=1,num do
		local r=1+flr(rnd(6))
		dices[i]={
			num=r,held=false,col=1}
	end
end

function drw_dice()
	for i=1,#dices do
		print(dices[i].num,
			52+(i-1)*10,42,dices[i].col)
	end
end
