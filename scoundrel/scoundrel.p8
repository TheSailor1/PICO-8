function drw_game()
	cls()
	
	--ui
	drw_hline(0,82,5)
	drw_vline(60,82,5)
	
	if #routines==0 then
		drw_deck(10,88)
		drw_hand()
	end
	
	
	rect(
		handpos[5].x+10,
		handpos[5].y+13,
		handpos[5].x+10+22,
		handpos[5].y+13+18,
		1)
		
	if not hasrun and #hand==4 then
		? "flee",handpos[5].x+14,handpos[5].y+16,12
		? "room",handpos[5].x+14,handpos[5].y+24,12
	end
	
	if hasweapon!=nil then
		drw_fcard(70,90,hasweapon.lmt,hasweapon.lmtsuit)
		line(81,88,81,118,0)
		pset(82,119,0)
		pset(83,120,0)
		line(84,121,101,121,0)
		drw_fcard(83,88,hasweapon.rank,hasweapon.suit)
	end
	
	? "hp___"..hp,93,10,8
	
	local newx=94
	local newy=12
	for i=0,maxhp-1 do
		if i%4==0 then
			newx=94
			newy+=6
		end
		
		? "♥",newx,newy,13
		newx+=6
	end
	
	local newx=94
	local newy=12
	for i=0,hp-1 do
		if i%4==0 then
			newx=94
			newy+=6
		end
		
		palt(7,true)
		sspr(119,0,6,5,newx+1,newy)
		palt()
		newx+=6
	end
	
	
	if #routines==0 and 
				#hand>0 then
		palt(0,false)
		drw_cursor()
	
	if cursel!=5 then
		drw_icos(handpos[cursel].x,
											handpos[cursel].y,
											hand[cursel])
	end
	end
	
	palt()
	--frame
	for i=0,2 do
		rect(0+i,0+i,127-i,127-i,6)
	end
end

function drw_deck(_x,_y)
	drw_bcard(_x,_y)
	? #shfdeck,_x+36,_y+20,12
--	? "room:"..room,_x+8,_y+26,7
end

function drw_hand()
	for i=1,#hand do
		drw_fcard(
			handpos[i].x,
			handpos[i].y,
			hand[i].rank,
			hand[i].suit
			)
	end
end

function drw_cursor()
	local movy
	if #routines==0 then
		movy=sin(time()/1.2)*0.2
	else
		movy=0
	end
	
	palt(7,true)
	sspr(
		24,27,
		21,22
		,handpos[cursel].x+22,
		handpos[cursel].y+20+movy)
	palt()
end

function drw_gameover()
	cls(8)
	? "game over!",32,32,7
end
