--cards

function mk_deck()
	for i=1,#suits do
		for j=2,14 do
			add(deck,{
				suit=suits[i],
				rank=j
			})
		end
	end
end

function set_deck()
	for i=#deck,1,-1 do
		if deck[i].suit=="hrt" or
					deck[i].suit=="dmd" then
			if deck[i].rank>=11 then
				deli(deck,i)
			end
		end
	end
end

function shuffle()
	for i=1,#deck do
		local r=1+flr(rnd(#deck))
		add(shfdeck,deck[r])
		deli(deck,r)
	end
end

function tak_cards(_v)
	sfx(60)
	for i=1,_v do
		add(hand,shfdeck[i])
		deli(shfdeck,i)
	end
	room+=1
	if hasrun==true then
		hasrun=false
	end
end

function flee()
	sfx(60)
	for i=4,1,-1 do
		add(shfdeck,hand[i])
		deli(hand,i)
	end
	tak_cards(4)
	room-=1
	hasrun=true
	cursel=1
end
