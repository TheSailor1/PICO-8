function _init()
	deck={}
	shfdeck={}
	suits={"hrt","dmd","clb","spd"}
	hand={}
	handpos={
		{x=10,y=8},
		{x=52,y=8},
		{x=10,y=46},
		{x=52,y=46},
		{x=84,y=40}
	}
	maxhand=4
	hasrun=false
	hp=20
	maxhp=20
	hasweapon=nil
	room=0
	
	cursel=1
	
	mk_deck()
	set_deck()
	shuffle()
	tak_cards(4)
	
	--ani
	routines={}
	hitx,hity,hitf,hitp=0,0,0,0
	async(deckani)


	_upd=upd_game
	_drw=drw_game
end

function _update60()
	_upd()
end

function _draw()
	_drw()
	
	for r in all(routines) do
		if costatus(r)=="dead" then
			del(routines,r)
		else
			coresume(r)
		end
	end
end
