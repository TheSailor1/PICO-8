--updates
function upd_game()
	if #routines==0 then
	--gameover
	if hp<=0 then
		_upd=upd_gameover
		_drw=drw_gameover
	end
	
	if btnp(❎) then
		if cursel==5 and #hand==4 then
			--flee room
			flee()
		else
			if hand[cursel].suit=="spd" or
					hand[cursel].suit=="clb" then
						--attack monster
						ax=handpos[cursel].x+32
						ay=handpos[cursel].y
						af=40
						
						if hasweapon then
							if hasweapon.lmt<hand[cursel].rank then
								--weapon lmt too low = bare fist
								sfx(61)
								
								ap=hand[cursel].rank
								async(dmgflash)
								
								hp-=hand[cursel].rank
								for i=1,hand[cursel].rank do
									del(hearts)
								end
								deli(hand,cursel)
							else
								sfx(61)
								hitpoints=hasweapon.rank-hand[cursel].rank
								if sgn(hitpoints)==-1 then
									hitpoints*=-1
									hp-=hitpoints
									ap=hitpoints
									async(dmgflash)
								end
								
								hasweapon.lmt=hand[cursel].rank
								hasweapon.lmtsuit=hand[cursel].suit
								
								for i=1,hand[cursel].rank do
									del(hearts)
								end
								deli(hand,cursel)
							end
						else
							--bare fists
							sfx(61)
							hp-=hand[cursel].rank
							ap=hand[cursel].rank
							async(dmgflash)
							for i=1,hand[cursel].rank do
									del(hearts)
							end
							deli(hand,cursel)
						end
		elseif hand[cursel].suit=="hrt" then
			--drink potion
			sfx(59)
			hp+=hand[cursel].rank
			deli(hand,cursel)
		elseif hand[cursel].suit=="dmd" then
			--take weapon
			sfx(62)
			hasweapon=hand[cursel]
			hasweapon.lmt=14
			hasweapon.lmtsuit="clb"
			deli(hand,cursel)
		end
		
		--keep maxhp
		if hp>maxhp then
			hp=maxhp
		end
		
		cursel=1
		
		--redraw
		if #hand==1 then
			tak_cards(3)
		end
		
		
		end
	end
	
	if btnp(🅾️) then
		
	end
	
	upd_cursor()
	end
end

function upd_cursor()
	local current=cursel
	
	if #hand>0 then
		--right
		if btnp(1) then
			if cursel==4 and not hasrun then
				cursel=5
			elseif cursel==2 and 
										#hand==4 and
										not hasrun then
				cursel=5
			elseif cursel<#handpos and 
						cursel<#hand then
				cursel+=1
			else
				cursel=1
			end
		end
		
		--left
		if btnp(0) then
			if cursel>1 then
				cursel-=1
			else
				cursel=#hand
			end
		end
		
		--up
		if btnp(2) then
			if cursel==3 and #hand>=3 then
				cursel=1
			elseif cursel==4 and #hand==4 then
				cursel=2
			end
		end
		
		--down
		if btnp(3) then
			if cursel==1 and #hand>=3 then
				cursel=3
			elseif cursel==2 and #hand==4 then
				cursel=4
			end
		end
	end
	
	if current!=cursel then
		sfx(63)
	end
	
end

function upd_gameover()
	
end
