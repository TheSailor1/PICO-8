function upd_game()
	if rolls>0 then
		if btnp(‚û°Ô∏è) and not newround and highdice<3 then
			highdice+=1
			sfx"63"
		end
		if btnp(‚¨ÖÔ∏è) and not newround and highdice>1 then
			highdice-=1
			sfx"63"
		end
	end
	
	if btnp(‚¨ÜÔ∏è) and 
	not dices[highdice].sel and
	not newround then
		dices[highdice].sel=true
		dicesel=true
		sfx"62"
	end
	if btnp(‚¨áÔ∏è) and 
	dices[highdice].sel and
	not newround then
		dices[highdice].sel=false
		sfx"61"
		mak_sparks(
			12,
			dices[highdice].x+16,
			dices[highdice].y+32,
			1,0.3,{2,4,5})
	end
	if btnp(üÖæÔ∏è) and not newround then
		rolls=0
	end
	
	--roll dice
	local cnt=0
	for i=1,3 do
		if dices[i].sel then
			cnt+=1
		end
	end
	if cnt==0 then
		dicesel=false
	end
	if btnp(‚ùé) then
		if dicesel  or newround then
			for i=1,3 do
				if dices[i].sel then
					rollfx(0,dices[i].x+16,dices[i].y)
					mak_sparks(
						122,
						dices[i].x+16,
						dices[i].y,
						3,0.6,{4,9,10})
				end
			end
			rolldice()
		end
	end
	
	--check for six
	local sixes={}
	for i=1,3 do
		if not newround and
		dices[i].sel and 
		dices[i].num==6 then
			add(sixes,highdice)
		end
	end
	
	
	if #sixes>1 then
		sfx"59"
		dices[sixes[1]].num=1
		for i=1,3 do
			dices[i].sel=false
		end
		gethand()
		sixes={}
	end
	
	if rolls==0 then
		checkwin()
	end
	
--	if rolls==0 and not roundend then
--		roundend=true
--		rolls=3
--		for i=1,3 do
--			dices[i].sel=true
--			newround=true
--		end
--	end
	
	
	upd_ens()
	upd_parts()
end
