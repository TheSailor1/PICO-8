function ini_parts()
	parts={}
	sparks={}
end

function rollfx(_t,_x,_y)
	--ring
	add(parts,{
		t=0,
		x=_x,
		y=_y,
		r=22,
		c=7,
		s=2
	})
	add(parts,{
		t=0,
		x=_x,
		y=_y,
		r=27,
		c=7,
		s=2
	})
	
	add(parts,{
		t=1,
		x=_x,
		y=_y,
		r=20,
		c=1,
		s=2
	})
end

function mak_sparks(_a,_x,_y,_s,_rs,_tbl)
	local amt=_a
	for i=1,amt do
		add(sparks,{
			x=_x,
			y=_y,
			c=rnd(_tbl),
			r=2+rnd(6),
			rs=_rs,
			dx=rnd(_s*2)-_s,
			dy=rnd(_s*2)-_s,
			age=rnd(30)
		})
	end
end

function upd_parts()
	for p in all(parts) do
		if p.t==0 then
			if p.r>0 then
				p.r-=p.s
			else
				del(parts,p)
			end
		elseif p.t==1 then
			if p.r>0 then
				p.r-=p.s
			else
				del(parts,p)
			end
		end
	end
	
	
	for s in all(sparks) do
		if s.age>0 then
			s.age-=1
			s.r-=s.rs
			s.x+=s.dx
			s.y+=s.dy
		else
			del(sparks,s)
		end
	end
	
end

function drw_parts()
	for p in all(parts) do
		if p.t==0 then
			circ(p.x,p.y,p.r,p.c)
		elseif p.t==1 then
			circfill(p.x,p.y,p.r,p.c)
			fillp(â–’)
			circfill(p.x,p.y,p.r,6)
			fillp()
		end
	end
	
	for s in all(sparks) do
		circfill(s.x,s.y,s.r,s.c)
	end
end
