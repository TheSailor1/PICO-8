function ini_game()
	
	--mkae dice
	dices={}
	curhand={}
	curhand2={}
	handscore=""
	handscore_txt=""
	rolls=3
	roundend=false
	newround=false
	dicesel=false
	hp=20
	
	for i=1,3 do
		add(dices,{
			x=13+((i-1)*35),
			y=62,
			sel=false,
			num=1+flr(rnd(6))
		})
	end
	
	--check hand
	gethand()
	highdice=2
	
	ini_ens()
	
	_upd=upd_game
	_drw=drw_game
end

