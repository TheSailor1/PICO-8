function drw_game()
	cls()
	
	--bkg
	ovalfill(4,80,124,110,3)
	sspr(96,0,23,36,1,76)
	sspr(86,35,74,34,104,79)
	
	drw_ens()
	
	--player dice
	for i=1,#dices do
		local ny=0
		if dices[i].sel then
			ny=20+sin((time()-(i*12))*1.2)*1.4
			ovalfill(
				38+((i-1)*35)+cos(time()),86,
				20+((i-1)*35)-cos(time()),88,
				1)
		else
			ovalfill(
				9+((i-1)*35),86,
				48+((i-1)*35),98,
				1)
		end
			
		
		sspr(64,0,32,31,dices[i].x,dices[i].y-ny)
		
		local ty=0
		for i=1,3 do
			if dices[i].sel then
				ty=20+sin((time()-(i*12))*1.2)*1.4
			else 
				ty=0
			end
			
			dots(
				dices[i].x+8,
				(dices[i].y+5)-ty,
				dices[i].num
				)
		end
	end
	
	local txt="\^t\^w"..handscore_txt
	print(txt,10,110,10)
	print("rolls: "..rolls,90,114,10)
	
	--cursor
	local oy=0
	for i=1,#dices do
		if dices[highdice].sel then
			oy=20
		end
	end
	
	if rolls>0 and not newround then
		local sx,sy
		if dices[highdice].num==1 then
			sx,sy=51,50
		else
			sx,sy=50,16
		end
		sspr(sx,sy,10,11,24+((highdice-1)*35),(48-oy)+sin(time()))
		pal()
	end
	
	local s,sc="",11
	if newround then
		s="new hand ‚ùé"
		sc=9
	elseif dicesel then
		s="roll again ‚ùé"
	else
		s="keep hand üÖæÔ∏è"
	end
	cprint(s,102,sc)
	
end
