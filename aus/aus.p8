function ini_ens()
	ens={}
	
	enroll()
	
	add(ens,20)
end

function enroll()
	enshand={}
	for i=1,3 do
		enshand[i]=1+flr(rnd(6))
	end
end

function upd_ens()
	if roundend then
		hiten()
		roundend=false
	end
end

function drw_ens()
	clip(7,9,39,39)
	rectfill2(rectfill,7,9,38,38,14)
	rectfill2(rectfill,8,10,36,36,2)
	circfill(26,38,12,1)
	clip()
	
	sspr(0,0,50,46,2,2)
	
	local ec,yc=9,9
	line(52,10,118,10,2)
	line(52,29,118,29,2)
	line(119,11,119,28,2)
	print("springer   ".."hp:"..tostr(ens[1]),54,22,ec)
	print("hp: "..tostr(hp),54,32,yc)
	
	for i=1,3 do
		sspr(50,0,14,16,40+(i*16),4)
		print(enshand[i],42+(i*17),6,1)
	end
end

function hiten()
	sfx"58"
	ens[1]-=1
end
