--tools

function cprint(_s,_y,_c)
	print(_s,64-(#_s*2),_y,_c)
end

function dots(_x,_y,_num)
		local sx
		if _num==6 then
			sx=0
		elseif _num==5 then
			sx=16
		elseif _num==4 then
			sx=32
		elseif _num==3 then
			sx=48
		elseif _num==2 then
			sx=64
		else
			sx=80
		end
		
		sspr(sx,106,16,22,_x,_y)
end

function rolldice()
	sfx"60"
	if rolls>0 then
		for i=1,3 do
			if dices[i].sel then
				dices[i].num=1+flr(rnd(6))
				dices[i].sel=false
			end
		end
		if not newround then
			rolls-=1
		end
		gethand()
	end
	newround=false
end

function bubsort_s(a)
	local itemcount=#a
	local haschanged
	repeat
		haschanged = false
		itemcount=itemcount - 1
		for i = 1, itemcount do
			if a[i] < a[i + 1] then
			a[i], a[i + 1] = a[i + 1], a[i]
			haschanged = true
			end
		end
	until haschanged == false
end

function bubsort_b(a)
	local itemcount=#a
	local haschanged
	repeat
		haschanged = false
		itemcount=itemcount - 1
		for i = 1, itemcount do
			if a[i] > a[i + 1] then
			a[i], a[i + 1] = a[i + 1], a[i]
			haschanged = true
			end
		end
	until haschanged == false
end


function gethand()
	curhand={}
	curhand2={}
	handscore=""
	for s=1,3 do
		if not dices[s].sel then
			add(curhand,dices[s].num)
			add(curhand2,dices[s].num)
		end
	end
	bubsort_s(curhand)
	bubsort_b(curhand2)
	
	for i=1,3 do
		handscore=handscore..tostr(curhand2[i])
	end
	
	if handscore=="122" then
		handscore_txt="julie"
	elseif handscore=="123" or
	handscore=="234" or
	handscore=="345" or
	handscore=="456" then
		handscore_txt="straat"
	elseif handscore=="112" or
	handscore=="113" or
	handscore=="114" or
	handscore=="115" or
	handscore=="116" then
		handscore_txt="maks"
	elseif handscore=="222" or
	handscore=="333" or
	handscore=="444" or
	handscore=="555" or
	handscore=="666" then
		handscore_txt="trip"
	elseif handscore=="111" then
		handscore_txt="aus!!"
	else
		handscore=""
		for i=1,3 do
		handscore=handscore..tostr(curhand[i])
		end
		handscore_txt=handscore
	end
end

function rectfill2(_s,_x,_y,_w,_h,_c)
	_s(_x,_y,_x+_w,_y+_h,_c)
end

