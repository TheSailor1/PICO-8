-- draws

function drw_bkg(_x,_y,_x2,_y2,_c)
	ovalfill(_x,_y,_x2,_y2,_c)
	sspr(96,0,23,35,_x-3,_y-2)
	sspr(86,35,22,35,_x+98,_y)
end

function drw_diceidle(_x,_y)
	--shadow
	ovalfill(_x-2,_y+22,
										_x+33,_y+35,1)
	sspr(64,0,32,31,_x,_y)
	
	--dots
	sspr(0,106,16,22,_x+8,_y+5)
end

function drw_dicechosen(_x,_y)
	--shadow
	ovalfill(_x+6,_y+46,
										_x+27,_y+51,1)
	sspr(64,0,32,31,_x,_y)
	
	--dots
	sspr(0,106,16,22,_x+8,_y+5)
end

function drw_dicechosen2(_x,_y)
	
	if dice_state==2 then
		sp_count+=timing
		if sp_count>=#sprites+1 then
			sp_count=1
			timing=0
		end
	end
	
	if timing>0 then
		?sprites[flr(sp_count)][1],_x+sprites[flr(sp_count)][2],_y+sprites[flr(sp_count)][3]
	else
		sspr(64,0,32,31,_x+2+cos(time())*1.2,_y-35+sin(time())*1.4)
		sspr(0,106,16,22,_x+10+cos(time())*1.2,_y-30+sin(time())*1.4)
	end
end


function drw_diceroll(_x,_y)
	--shadow
	ovalfill(_x+6,_y+46,
										_x+27,_y+51,1)
	sspr(64,0,32,31,_x,_y)
	
	--dots
	sspr(0,106,16,22,_x+8,_y+5)
end

function drw_diceconvert(_x,_y)
	--shadow
	ovalfill(_x+6,_y+46,
										_x+27,_y+51,1)
	sspr(64,0,32,31,_x,_y)
	
	--dots
	sspr(0,106,16,22,_x+8,_y+5)
end
