pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
function _init()
	dice_allstates=split"idle,chosen,roll,convert"
	dice_state=1
	dice_ox=45
	dice_oy=65
	dice_x=dice_ox
	dice_y=dice_ox
	
	runani=false
	antime=0
	
	sp_count=1
	otime=0.55
	timing=otime
end

function _update60()
	if antime>0 then
		antime-=1
	else
		antime=0 
	end 
	
	if dice_state==1 then
		dice_y=dice_oy
	elseif dice_state==2 then
	-- something
	elseif dice_state>=3 then
		if antime==0 then
			dice_state=1
		end
	end
	
	
	--idle
	if btnp(⬆️) and 
	dice_state==1 then
		sfx"0"
		dice_state=2
	end
	
	--chosen
	if btnp(⬇️) and 
	dice_state==2 then
		sfx"2"
		dice_state=1
	end
	
	--roll
	if btnp(❎) and
	dice_state==2 then
		sfx"1"
		dice_state=3
		antime=30
	end
	
	--convert
	if btnp(🅾️) and
	dice_state==2 then
		dice_state=4
		antime=30
	end
	
end

function _draw()
	cls()
	
	drw_bkg(7,64,122,98,3)
	
	sspr(64,0,32,31,20,52)
	sspr(64,0,32,31,70,52)
	sspr(0,106,16,22,28,57)
	sspr(0,106,16,22,78,57)
	
	if dice_state==1 then
		drw_diceidle(dice_x,dice_y)
		sp_count=1
		timing=otime
	elseif dice_state==2 then
		drw_dicechosen2(dice_x,dice_y)
	elseif dice_state==3 then
		drw_diceroll(dice_x,dice_y)
	elseif dice_state==4 then
		drw_diceconvert(dice_x,dice_y)
	end
	
	
	--★
	local c
	for i=1,#dice_allstates do
		if i==dice_state then
			c=7
		else
			c=8
		end
		?dice_allstates[i],1,1+(i*8),c
	end
	
	?antime,1,100,9

end
