function ini_parts()
	parts={}
end--ini_parts()

function upd_parts()
	local p
	for p in all(parts) do
		p.x+=p.vx
		p.y+=p.vy
		
		if p.t==0 then
			p.r-=0.5
		elseif p.t==1 then
			p.r-=0.1
		end
		
		if p.age<p.maxage then
			p.age+=1
		else
			del(parts,p)
		end
	end--for
end--upd_parts()

function drw_parts()
	local p
	for p in all(parts) do
		if p.t==0 then
			circ(p.x,p.y,p.r,p.c)
		elseif p.t==1 then
			circfill(p.x,p.y,p.r,p.c)
		end--if p.t
	end--for
end--drw_parts()

