function ini_plr()
	plr={
		x=16,
		y=80,
		sp=1,
		dir=1,
		lives=4,
		maxlives=4,
		inv=0,
		w=16,
		h=16,
		vx=0,
		vy=0,
		djump=false,
		jcount=0,
		jumpheight=3.5,
		grav=0.25,
		spd=2,
		frc=1.6,
		frc2=2,
		state="fall",
		onground=false,
		jumpbuff=false,
		bufftmr=0,
		ani_run={5,7},
		ani_idle={1,3},
		ani_jump={9,11},
		ani_fall={11,9},
		ani_hurt={13,13,13,13}
	}
	
end--ini_plr()

function drw_plr()
	local _pani
	
	if plr.state=="jump" then
		_pani=9
		if plr.dir==1 then
			plr.fx=false
		else
			plr.fx=true
		end
	elseif plr.state=="fall" then
		_pani=11
	elseif plr.state=="run" and plr.onground then
		_pani=ani_spr(plr.ani_run,7)
	elseif plr.state=="idle" then
		_pani=ani_spr(plr.ani_idle,50)
		plr.fx=false
	elseif plr.state=="hurt" then
		_pani=ani_spr(plr.ani_hurt,7)
	end--if
	
	if plr.inv>0 and plr.inv%5==0 then
		pal(1,8)
		pal(8,14)
		pal(9,8)
		pal(2,14)
		pal(15,8)
		pal(11,14)
		pal(4,14)
		pal(6,8)
		pal(14,8)
	end
	
	spr(_pani,plr.x,plr.y,2,2,plr.fx,false)
end--drw_plr()

function upd_plr()
	local lastx,lasty=plr.x,plr.y
	
	if plr.inv>0 then
		plr.inv-=1
	else
		plr.inv=0
	end
	
	if plr.bufftmr>0 then
		plr.bufftmr-=1
	end
	
	--add gravity
	plr.vy+=plr.grav
	if plr.vy>0.25 then
		plr.state="fall"
		plr.onground=false 
	end
	
	--get player inputs
	if btn(‚û°Ô∏è) then
		if plr.onground then
			plr.state="run"
			if camx<camx_max and plr.vx>0 then
				camx+=1
			end
		end
		plr.vx=plr.spd
		plr.fx=false
		plr.dir=1
	end--if
	if btn(‚¨ÖÔ∏è) then
		
		if plr.onground then
			plr.state="run"
		end
		plr.vx=-plr.spd
		plr.fx=true
		plr.dir=-1
	end--if
	
	-- player jump input
	if btnp(üÖæÔ∏è) then
				sfx(9)
	   if plr.onground==true then
					plr.state="jump"
					plr.onground=false
					plr.vy=-plr.jumpheight
					add_dust_land()
					plr.jcount+=1
				elseif plr.vy<0 then
					plr.bufftmr=10
				elseif plr.state=="fall" then
					if plr.jcount==0 then
						plr.bufftmr=10
					elseif plr.djump==true then
						sfx(10)
						add_dust() --dust particles
						plr.djump=false
						plr.vy=-plr.jumpheight
					end
				end
	end
	
	
	--add friction
	if plr.state=="jump" then
		plr.vx/=plr.frc2
	else
		plr.vx/=plr.frc
	end--if
	
	--update pos
	plr.x+=plr.vx
	plr.y+=plr.vy
	
	x0=flr((plr.x)/8)
	x1=flr((plr.x+2)/8)
	x2=flr((plr.x+7)/8)
	x3=flr((plr.x+9)/8)
	x4=flr((plr.x+13)/8)
	x5=flr((plr.x+16)/8)
	y1=flr((plr.y)/8)
	y2=flr((plr.y+4)/8)
	y3=flr((plr.y+8)/8)
	y4=flr((plr.y+12)/8)
	y5=flr((plr.y+15)/8)
	
	--collision with floor
	if fget(mget(x2,y5),0) or 
			fget(mget(x3,y5),0) then
		plr.vy=0
		plr.y=lasty
		plr.onground=true
		plr.jcount=0
		plr.djump=true -- can use djump again
		if plr.bufftmr>0 and
				plr.onground==true then
			plr.bufftmr=0
			plr.vy=-plr.jumpheight
		end
		if plr.vx==0 then
			plr.state="idle"
		elseif plr.vx~=0 then
			plr.state="run"
		end
		elseif plr.vy>1.7 then
			plr.state="fall"
		elseif plr.vy<0 then
			plr.state="jump"
	end--if
	
	
	--if ledge to right
	if fget(mget(x5,y5),0) and 
	  not fget(mget(x4,y5),0) and 
			not fget(mget(x3,y5),0) and
			not fget(mget(x2,y5),0) and
			not fget(mget(x1,y5),0) and
			not fget(mget(x0,y5),0) then
		plr.vx-=1
	end
	--if ledge to left
	if fget(mget(x0,y5),0) and 
	  not fget(mget(x1,y5),0) and 
			not fget(mget(x3,y5),0) and
			not fget(mget(x4,y5),0) then
		plr.vx+=1
	end
	
	--head collision
	if plr.vy<0 and
				fget(mget(x1,y1),0) and
				fget(mget(x2,y1),0) or
				fget(mget(x3,y1),0) then
		--player bumped head
		plr.vy*=0.2
	end--if
	
	--collision on right 
	if btn(‚û°Ô∏è) and
			fget(mget(x4,y1),0) or
			fget(mget(x4,y2),0)  or
			fget(mget(x4,y3),0)  or
			fget(mget(x4,y4),0)  then
		plr.vx=-plr.vx/8
		plr.x=lastx
	end--if
	
	--collision on left 
	if btn(‚¨ÖÔ∏è) and
			fget(mget(x1,y1),0) or
			fget(mget(x1,y2),0)  or
			fget(mget(x1,y3),0)  or
			fget(mget(x1,y4),0) then
		plr.vx=-plr.vx/8
		plr.x=lastx
	end--if
	
	if btn(‚û°Ô∏è) and
				fget(mget(x1,y1),0) and
				fget(mget(x1,y2),0) then
			plr.x+=1
	end--if
	if btn(‚¨ÖÔ∏è) and
				fget(mget(x5,y1),0) and
				fget(mget(x5,y2),0) then
			plr.x-=1
	end--if
	
	if not btn(‚¨ÖÔ∏è) and not btn(‚û°Ô∏è) then
		plr.vx=0
	end
	
	if collision_tile_dmg(plr,dir) then
			plr.state="hurt"
		if plr.inv==0 then
			sfx(12)
			camshake=10
			plr.lives-=1
			plr.inv=60
		end
	end
	
	local e
	for e in all(enemies) do
		if collision_with_enemy(plr,e) then
			plr.state="hurt"
			if plr.inv==0 then
				sfx(12)
				camshake=10
				plr.lives-=1
				plr.inv=60
			end
		end
	end
	
end--upd_plr()

function collision_tile_dmg(_p)
	local p_x1=(_p.x+6)/8
	local p_x2=(_p.x+_p.w-6)/8
	local p_y1=(_p.y+5)/8
	local p_y2=(_p.y+_p.h-4)/8
	
	dir=0
	
	if fget(mget(p_x1,p_y1),1) then
		dir=-1
		return true,dir
	end
	if fget(mget(p_x2,p_y1),1) then
		dir=1
		return true,dir
	end
	if fget(mget(p_x1,p_y2),1) then
		dir=-1
		return true,dir
	end
	if fget(mget(p_x2,p_y2),1) then
		dir=1
		return true,dir
	end
end

function collision_with_enemy(_p,_e)
	local hit=false
	
	local pw=10
	local ph=8
	local ew=10
	local eh=8
	
	local xs=pw*0.5+ew*0.5
	local ys=ph*0.5+eh*0.5
	local xd=abs((_p.x+(pw/2))-(_e.x+(ew/2)))
	local yd=abs((_p.y+(ph/2))-(_e.y+(eh/2)))
	
	if xd<xs and yd<ys then
		hit=true
	end
	
	return hit
end
