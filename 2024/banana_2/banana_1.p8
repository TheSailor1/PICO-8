function ini_enemies()
	enemies={}
	
	koalas={64,66}
end

function new_enemy(_x,_y,_t)
	e={
		x=_x*8,
		y=_y*8,
		w=16,
		h=16,
		dx=0,
		dy=0,
		spd=0.24,
		typ=_t
	}
	add(enemies,e)
end

function upd_enemies()
	local e
	for e in all(enemies) do
		local elasty=e.y
		e.dy+=grav
		
		e.x+=e.spd
		
		
		local ex1=(e.x+2)/8
		local ex2=(e.x+e.w-1)/8
		local ey=(e.y+e.h)/8
		local ey2=(e.y+8)/8
		if fget(mget(ex1,ey),0) or
					fget(mget(ex2,ey),0) then
		e.dy=0
		e.y=elasty
		end--if
		if fget(mget(ex1,ey2),0) then
		e.spd=-e.spd
		end--if
		if fget(mget(ex2,ey2),0) then
		e.spd=-e.spd
		end--if
		
		e.y+=e.dy
	end--for
end--upd_enemies

function drw_enemies()
	local eani
	for e in all(enemies) do
		if e.x<=plr.x+100 then
			if e.typ==0 then
				eani=ani_spr(koalas,20)
			end
		
			spr(eani,e.x,e.y,2,2)
		end
	end--for
end--drw_enemies
