--tools--

function cprint(_t,_y,_c)
	print(
		_t,
		63-(#_t*2),
		_y,
		_c
	)
end--cprint

function ani_spr(arr,spd)
	return arr[flr(t/spd)%#arr+1]
end

function replace_map_tiles()
	if ec==koalas then
		return
	end--if ec
	
	local i,j,ec=0,0,0
	
	for i=0,127 do
		for j=0,15 do
			if mget(i,j)==96 then
				mset(i,j,0)
				new_enemy(i,j,0)
				ec+=1
			elseif mget(i,j)==97 then
				mset(i,j,0)
				new_heart(i,j)
			elseif mget(i,j)==112 then
				mset(i,j,0)
				new_banana(i,j)
			end--if mget
		end--for j
	end--for i
end


function new_heart(_x,_y)
	local h={
		x=_x*8,
		y=_y*8
		}
	add(hearts,h)
end

function new_banana(_x,_y)
	local b={
		x=_x*8,
		y=_y*8
		}
	add(bananas,b)
end


function drw_hearts()
	local h
	for h in all(hearts) do
		if h.x<=plr.x+100 then
			spr(16,h.x+4,h.y+sin(time()/2)*3)
		end
	end
end

function drw_bananas()
	local b
	for b in all(bananas) do
		if b.x<=plr.x+100 then
			spr(70,
							b.x+4,
							b.y-4+sin(time()/2)*3,
							2,2)
		end
	end
end

function upd_hearts()
	local h
	for h in all(hearts) do
		if collision_with_enemy(plr,h) then
			sfx(13)
			del(hearts,h)
			if plr.lives<plr.maxlives then
				plr.lives+=1
			end
			msg_tmr=15
			msg="+‚ô•"
			msgx=plr.x+10
			msgy=plr.y
		end
	end 
end

function float_msg(_t,_x,_y,_c)
	
	if msg_tmr>0 then
		msg_tmr-=0.5
		floaty+=0.5
		print(_t,_x+2,_y+2-floaty,1)
		print(_t,_x+1,_y+1-floaty,14)
		print(_t,_x,_y-floaty,_c)
	end
	
end--float_msg()

function upd_bananas()
	local b
	for b in all(bananas) do
		if collision_with_enemy(plr,b) then
			sfx(11)
			del(bananas,b)
			points+=1
			add_fanfare()
			msg_tmr=15
			msg="+1!"
			msgx=plr.x+10
			msgy=plr.y
		end
	end 
end

