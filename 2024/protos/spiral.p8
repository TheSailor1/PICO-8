pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
function _init()
	r=4
	t=0
end

function _update60()
	t=t-1/90
end

function _draw()
	cls()
	for i=0,6,1/128 do
		circfill(
			64+r*(4*i+1)*cos(i+t),
			64+r*(4*i+1)*sin(i+t),
			r,8)
		circfill(
			64+r*(4*i+1)*cos(i+t),
			64+r*(4*i+1)*sin(i+t),
			r,2)
	end
end
