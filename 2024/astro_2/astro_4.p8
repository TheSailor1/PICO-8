		grav=0
		plrdy=0
		plry=ground
		onground=true
		if (oxyboost>0) oxyboost-=0.1
		if moving then
			plrstat="rn"
		else
			plrstat="st"
		end
	else
		grav=0.14
		onground=false
	end
	if (plrdy>0) plrstat="fl"
end


