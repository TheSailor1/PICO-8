-- tools

function new_scn(_u,_d)
	_update60=_u
	_draw=_d
end

function upd_tick()
	tick+=1
	if tick>tick_max then
		new_meteor()
		tick=0
		tick_max=200+rnd(500)
	end
end

function upd_wtmr(_t)
	if wtmr>0 then
		wtmr-=1
	else
		wtmr=0
	end
end

function upd_tmr()
	if tmr>31000 then
		tmr=0
	else
		tmr+=1
	end
end

function upd_anitmr()
	if anitmr<1 then
		anitmr+=0.008
	else
		anitmr=1
	end
end

function lerp(_f,_t,_w,_p)
	local d=_f-_t
	if d<_p then
		return 0
	else
		return (_t-_f)/_w
	end
end

function easeinquad(_t)
	return _t*_t
end

function easeinoutquad(t)
	if(t<.5) then
		return t*t*2
	else
		t-=1
		return 1-t*t*2
	end
end

function easeoutbounce(t)
	local n1=7.5625
	local d1=2.75
	
	if (t<1/d1) then
		return n1*t*t;
	elseif(t<2/d1) then
		t-=1.5/d1
		return n1*t*t+.75;
	elseif(t<2.5/d1) then
		t-=2.25/d1
		return n1*t*t+.9375;
	else
		t-=2.625/d1
		return n1*t*t+.984375;
	end
end

function rrect(_x,_y,_w,_h,_c,_c2)
	rectfill(_x,_y,_x+_w,_y+_h,_c)
	rectfill(_x+1,_y-1,_x+_w-1,_y+_h+1,_c)
	line(_x+1,_y+_h+2,_x+_w-1,_y+_h+2,_c2)
end

function rrect2(_x,_y,_w,_h,_c)
	rect(_x,_y,_x+_w,_y+_h,_c)
	rect(_x+1,_y-1,_x+_w-1,_y+_h+1,_c)
	
	rectfill(_x+1,_y,_x+_w-1,_y+_h,0)
end


