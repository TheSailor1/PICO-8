--particles

function ini_parts()
	stars={}
	air={}
end

function ad_star(_y,_amt)
	stars={}
	for i=1,_amt do
		add(stars, {
			x=64+rnd(64)-rnd(64),
			y=_y+rnd(64)-rnd(64),
			r=0,
			c=rnd({1,1,1,2,9,12})
		})
	end
end

function drw_star(_o)
	for s in all(stars) do
		circfill(s.x,s.y-_o,s.r,s.c)
	end
end

function ini_meteors()
	meteors={}
end

function new_meteor()
	add(meteors,{
		x=rnd(127),
		y=rnd(80),
		c=rnd({1,13,12}),
		r=rnd({0,1}),
		spdx=rnd({-3,-2,-1,1,2,3}),
		spdy=rnd({-3,-2,-1,1,2,3})
	})
end

function upd_meteors()
	for m in all(meteors) do
		m.x+=m.spdx
		m.y-=m.spdy
		
		--if offscreen
		if m.y<0 or m.x<0 or
					m.y>80 or m.x>127 then
						del(meteors,m)
		end
	end
end

function drw_meteors()
	for m in all(meteors) do
		circfill(m.x,m.y,m.r,m.c)
	end
end

function useboost()
	for i=1,20 do
		add(air,{
			x=plrx,
			y=plry,
			dx=rnd(3)*-1,
			dy=rnd(1)*-1,
			mage=30+rnd(100),
			age=0,
			r=1+rnd(2)
		})
	end
	
	add(air,{
		x=plrx,
		y=plry+4,
		dx=0,
		dy=0,
		mage=10,
		age=0,
		r=3
		})
	
end

function drw_boost()
	for b in all(air) do
		circfill(b.x,b.y,b.r,rnd({11,10}))
	end
end

function upd_boost()
	for b in all(air) do
		if b.age<b.mage then
			b.age+=1
			b.r-=0.1
			b.x+=b.dx
			b.y+=b.dy
		else
			del(air,b)
		end
	end
end
