function drw_game()
	cls()
	drw_star(ly-20)
	
	?sp_p1,plx,100-ly
	drw_meteors()
	
	
	drw_bkg()
	
	--floor
	for i=0,1 do
		map(0,0,mapx-i*128,mapy,16,2)
	end
	
	--aliens
	for a in all(curaliens) do
		drw_alien(a.x,a.y)
	end
	
	if foundship then
		if (shpx-plrx)<30 then
			moving=false
		else
			shpx-=1
		end
		?sp_roc,shpx,-6
	end
	
	drw_ast(plrx,plry,plrstat)
	
	drw_hud()
	drw_boost()
	
	?#curaliens,2,2,8
end

