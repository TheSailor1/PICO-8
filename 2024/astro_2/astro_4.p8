-- astro

function drw_ast(_x,_y,_s)
	local bx,by,bw,bh,hx,hy,hw,hh
	local runlib={0,12,23,12}
	local runlibh={0,12,24,12}
	local jmplib={60,60,60,60}
	local jmplibh={12,12,12,12}
	local fallib={72,72,72,72}
	local fallibh={24,24,24,24}
	local bbounce,hbounce=0,0
	local xoff,yoff=0,0
	
	if _s=="st" then
		--stand
		bx,by,bw,bh=0,0,12,9
		hx,hy,hw,hh=0,11,12,10
		bbounce=0
		hbounce=sin(tmr*0.004)*0.003
	elseif _s=="ex" then
	--exclamation
		bx,by,bw,bh=12,0,12,9
		hx,hy,hw,hh=12,11,12,10
		bbounce=0
		hbounce=sin(tmr*0.004)*0.003
		xoff=3
		yoff=0
	elseif _s=="rn" then
		bx,by,bw,bh=runlib[flr(frcnt)],0,12,9
		hx,hy,hw,hh=runlibh[flr(frcnt)],11,12,10
		bbounce=sin(tmr*0.04)*0.3
		hbounce=sin(tmr*0.04)*0.03
		xoff=tmr\10%3
		yoff=0
	elseif _s=="jp" then
		bx,by,bw,bh=jmplib[flr(frcnt)],0,12,9
		hx,hy,hw,hh=jmplibh[flr(frcnt)],11,12,10
		bbounce=sin(tmr*0.4)*0.3
		hbounce=sin(tmr*0.004)*0.003
		xoff=3
		yoff=-2
	elseif _s=="fl" then
		bx,by,bw,bh=fallib[flr(frcnt)],0,12,9
		hx,hy,hw,hh=fallibh[flr(frcnt)],11,12,10
		bbounce=0
		hbounce=sin(tmr*0.004)*0.003
		xoff=0
		yoff=2
	end
	
	--body
	sspr(bx,by,bw,bh,_x,_y+bbounce)
	--head
	sspr(hx,hy,hw,hh,_x+1-xoff,(_y-7)+yoff+hbounce)
end

