-- aliens

function spawn_alien(_n)
	for i=1,
	allaliens[level][_n] do
		add(curaliens,{
			x=227+(i*24),
			y=64,
			dx=1,
			dy=0
			})
	end
end

function upd_aliens()
	for a in all(curaliens) do
		if a.x>-26 then
			a.x-=a.dx+oxyboost
		else
			del(curaliens,a)
			if #curaliens==0 then
				if aliencnt<#allaliens[level] then
					aliencnt+=1
					spawn_alien(aliencnt)
					mov_marker=true
				end
			end
		end
	end
end

function drw_alien(_x,_y)
	--body
	sspr(0,24,20,10,_x,_y)
	--head
	sspr(0,34,11,10,_x-2,(_y-5)+sin(tmr*0.002)*0.4)
end
