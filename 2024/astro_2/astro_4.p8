function upd_menu()
	upd_tmr()
	upd_wtmr()
	upd_anitmr()
	upd_tick()
	upd_meteors()
	
	if ly>118 and ly<119 and snd~=3 then
		sfx"61"
		snd+=1
	end
	
	if btnp(🅾️) then
		sfx"63"
		ini_intro()
	end
end

function upd_intro()
	upd_tmr()
	upd_wtmr()
	upd_anitmr()
	upd_tick()
	upd_meteors()
	
	if btnp(🅾️) and wtmr==0 and txt_cnt<#txt then
		txt_cnt+=1
		wtmr=40
		sfx"63"
		if txt_cnt==4 or txt_cnt==6 then
			sfx"62"
		elseif txt_cnt==#txt then
			ini_game()
		end
	end
end

function upd_game()
	upd_tmr()
	if moving then
		plx-=(mapspd/100)
		mapx=(mapx-1-oxyboost)%128
	end
	
	
	upd_tick()
	upd_meteors()
	upd_boost()
	
	plr_inputs()
	upd_plr()
	upd_hud()
	
	upd_aliens()
	
	
	if frcnt<5.9 then
		frcnt+=.22
	else
		frcnt=1
	end
end

function upd_hud()
	t+=1
	if (fline_x-marker_x)<2 then
		foundship=true
		marker_x=fline_x
	end
	
	if (timer<otime*0.3 and t%10000<2) sfx"57"
	
	if btnp(❎) and oxy>0 then
		sfx"58"
		boost=(distance*0.10)*scale
		oxy-=1
		oxyboost=3
		useboost()
	end
	
	if #curaliens==0 then
		foundship=true
	end
				timer=flr))
	
	if not foundship then
		if mov_marker then
			mov_marker=false
			if (oxyboost>0) oxyboost=0
			if timer>0 then
--				timer-=speed
				marker_x+=(speed+boost)*scale
				boost=0
			else
				marker_x=fline_x
			end
		end
	end--foundship
end
