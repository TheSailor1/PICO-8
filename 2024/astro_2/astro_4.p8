		rrect(bub_x[txt_cnt],bub_y[txt_cnt],bub_w[txt_cnt],bub_h[txt_cnt],bub_c[txt_cnt],bub_c2[txt_cnt])
		print(txt[txt_cnt],txt_x[txt_cnt],txt_y[txt_cnt],txt_c[txt_cnt])
