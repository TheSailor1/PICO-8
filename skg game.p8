--collision

function collide_map(obj,aim,flag)
	--obj=table needs x,y,w,h
	local x=obj.x*8   local y=obj.y*8
	local w=obj.w   local h=obj.h
--	x1=0
--	y1=0
--	x2=0
--	y2=0
	if aim=="left" then
		x1=x+w-1  y1=y
		x2=x+w-1    y2=y+h-1
	elseif aim=="right" then
		x1=x+w-1   y1=y
		x2=x+w-1      y2=y+h-1
	elseif aim=="up" then
		x1=x+2  y1=y
		x2=x+w-3  y2=y
	elseif aim=="down" then
		x1=x+2		  y1=y+h-1
		x2=x+w-3   y2=y+h-1
	end
	
	
	
	x1r=x1  y1r=y1
	x2r=x2  y2r=y2
	

	
	
	
	
	
	x1/=8 y1/=8
	x2/=8 y2/=8

	if fget(mget(x1,y1), flag)
	or fget(mget(x1,y2), flag)
	or fget(mget(x2,y1), flag)
	or fget(mget(x2,y2), flag) then
		return true
	else
		return false
	end
	
end
