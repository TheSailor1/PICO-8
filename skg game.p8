function doshake()

	local shakex=16-rnd(48)
	local shakey=16-rnd(48)
	shakex=shakex*shake
	shakey=shakey*shake
		
	camera(shakex,shakey)
		shake*=0.96
		shake*=0.96

		if shake <0.05 then
			shake=0
			camera(mapx*8,mapy*8)
		end
end

