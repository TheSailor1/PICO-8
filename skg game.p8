	shake=0
	
	dirx={-1,1,0,0}
	diry={0,0,-1,1}
	
	t=0
	stimer=0
	dtimer=180
	dtimerend=90
	anim_timer=150
	bar=72
	og=38
	ogp=27
	fn=34
	fnp=57
	rg=14
	rgp=91
	gg=44
	cb=32
	
talkwind=nil

wind={}

_upd=update_start
_drw=draw_start
	
end

function _update()

	t+=1

if not game_over then
			_upd()
	else
		if btnp(❎) then
		 extcmd("reset")
		end
	end

		
end

function _draw()

	_drw()
	rect(x1r,y1r,x2r,y2r,7)

	drawind()
--addwind(15,80,100,30,{"its been a while."})


end
