function draw_player()
	spr(p.spr,p.x *8,p.y*8,1,1,p.flip)
		if is_tile(win,p.x,p.y) then
			animate_death()
	end	
end









function move_player()
p.ox=p.x
p.oy=p.y
	if btnp(⬅️) then
		p.x-=1
		p.spr+=1
		if p.spr>5 then
			p.spr=4
		end
		p.flip=true
		if	collide_map(p,"left",0) then
			p.x=p.ox
		end
	end
	
	if btnp(➡️) then
		p.x+=1
		p.spr+=1
		if p.spr>5 then
			p.spr=4
		end
		p.flip=false
		
		if	collide_map(p,"right",0) then
			p.x=p.ox
		end
	end
	
	if btnp(⬆️) then
		p.y-=1.5
		if	collide_map(p,"up",0) then
			p.y=p.oy
		end
	end
	
	
	if btnp(⬇️) then
		p.y+=1
		if	collide_map(p,"down",0) then
			p.y=p.oy
		end
	end


	if can_move(p.x,p.y) then
			p.x=mid(0,p.x,127)
			p.y=mid(0,p.y,127)
	end


end


	
function interact(x,y)

	if is_tile(key,x,y) and btnp(❎) then
		get_key_bomb(x,y)
		shake=1
	--elseif is_tile(key,x,y) then
				--whatever
	elseif is_tile(door,x,y) and p.keys>0 then
			open_door(x,y)
	end
end
