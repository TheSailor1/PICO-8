function map_setup()
--timers
 timer=0
 anim_time=30
 
 
	wall=0
	key=1
	door=2
	no_jump=3
	idk=4
	text=5
	friend=6
	win=7
end

function update_map()
	if timer<0 then
		--toggle_tiles2()
		timer=anim_time
	end
	timer-=1

end

function draw_map()
	mapx=flr(p.x/16)*16
	mapy=flr(p.y/16)*16
	if not is_tile(key,80,42) and btnp(❎) then
		camera(mapx*8,mapy*8)
else
		doshake()
	end
	map(0,0,0,0,128,64)
	
	
end

function is_tile(tile_type,x,y)
	tile= mget(x,y)
	has_flag=fget(tile,tile_type)
	return has_flag
end


function can_move(x,y)
	return not is_tile(wall,x,y)
end


function swap_tile(x,y)
	tile=mget(x,y)
	mset(x,y,tile+1)
end

function unswap_tile(x,y)
	tile=mget(x,y)
	mset(x,y,tile-1)
end

function swap_tile2(x,y)
	tile=mget(x,y)
	mset(x,y,tile+2)
end


function unswap_tile2(x,y)
	tile=mget(x,y)
	mset(x,y,tile-2)
end

function get_key_bomb(x,y)
	p.keys+=1
	swap_tile(x,y)
	sfx(1)
end

function get_key_no_tile()
	p.keys+=1
end

function open_door(x,y)
	p.keys-=1
	swap_tile(x,y)
	sfx(0)
end


