function make_player()
--	p_ani={1,2}
	p={}
	--p.x=116
	--p.y=27
	 p.x=0
		p.y=5
		
		p.ox=0
		p.oy=0
		
		p.w=8
		p.h=8
		boost=4
--		p.xv=0
--		p.yv=0
--		p.max_xv=2
--		p.max_yv=3
		
		
--		p.acc=0.5
--		p.boost=4
--		p.anima=0
		
		p.spr=4
		p.keys=0
		p.flip=false
		
		p.walk=false
		p.ladder=false
		p.falling=false
		p.idle=false
		p.landed=false
