--ui

function addwind(_x,_y,_w,_h,_txt)
	local w={x=_x,
							y=_y,
							w=_w,
							h=_h,
							txt=_txt}
	add(wind,w)
	return w
end


function rectfill2(_x,_y,_w,_h,_c)
	rectfill(_x,_y,_x+max(_w-1,0),_y+max(_h-1,0),_c)
end

function drawind()
	for w in all(wind) do
		local wx,wy,ww,wh=w.x,w.y,w.w,w.h
		rectfill2(wx,wy,ww,wh,13)
		rectfill2(wx+1,wy+1,ww-2,wh-2,0)
		wx+=4
		wy+=4
		clip(wx,wy,ww-8,wh-8)
		for i=1, #w.txt do
			local txt=w.txt[i]
			print(txt,wx,wy,6)
			wy+=6
		end
		
		if w.dur!=nil then
			w.dur-=1
			if w.dur<=0 then
				local dif=wh/4
				w.y+=dif/21
				w.h-=dif
				if wh<3 then
					del(wind,w)
					end
			end
		end
	end
end

